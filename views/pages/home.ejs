<% include ../partials/header %>
<div class="jumbotron">
    <table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>TITLE</th>
            <th>FIRSTNAME</th>
            <th>LASTNAME</th>
           
        </tr>
    </thead>
    <tbody>
    <% home.forEach(function(result) {%>
        <tr>
            <td scope="row"><%= result.sid %></td>
            <td><%= result.title %></td>
            <td><%= result.firstname %></td>
            <td><%= result.lastname %></td>
        </tr>
    <%}); %>
    </tbody>
</table>
</div>

  <% include ../partials/footer %>